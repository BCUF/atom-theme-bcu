
<section>
  <h3><?php echo $browseMenu->getLabel() ?></h3>
  <ul>
    <?php foreach ($browseMenu->getChildren() as $item): ?>
      <li>
        <a href="<?php echo url_for($item->getPath(array('getUrl' => true, 'resolveAlias' => true))) ?>">
          <?php echo $item->getLabel(array('cultureFallback' => true)) ?>
        </a>
      </li>
    <?php endforeach; ?>
  </ul>
</section>

<?php echo get_component('menu', 'staticPagesMenu') ?>
        
        <section>
          <!-- <h2></?php echo __('Browse by') ?></h2> -->
          <ul class="BCU-header-menu-ul">
            <?php $browseMenu = QubitMenu::getById(QubitMenu::BROWSE_ID) ?>
            <?php if ($browseMenu->hasChildren()): ?>
              <?php foreach ($browseMenu->getChildren() as $item): ?>
                <li class="BCU-header-menu-li"><a href="<?php echo url_for($item->getPath(array('getUrl' => true, 'resolveAlias' => true))) ?>"><?php echo esc_specialchars($item->getLabel(array('cultureFallback' => true))) ?></a></li>
              <?php endforeach; ?>
            <?php endif; ?>
          </ul>
        </section>